{
    "name": "aura/sqlmapper-bundle",
    "type": "library",
    "description": "A SQL DataMapper implementation.",
    "keywords": [ "sql", "data", "mapper" ],
    "homepage": "http://auraphp.github.com/Aura.SqlMapper_Bundle",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura Contributors",
            "homepage": "https://github.com/auraphp/Aura.SqlMapper_Bundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "aura/sql": "~2.0",
        "aura/sqlquery": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Aura\\SqlMapper_Bundle\\": "src/"
        }
    },
    "extra": {
        "aura": {
            "type": "bundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aura\\Sql\\": "vendor/aura/sql/tests/",
            "Aura\\SqlMapper_Bundle\\": "tests/"
        }
    }
}
